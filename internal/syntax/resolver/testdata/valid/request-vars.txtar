# A single HTTP request with some variables and keywords

-- src.http --
###
# @name MyRequest
# @timeout 30s
# @connection-timeout 10s
# @no-redirect
# @normal-var = test
GET https://example.com
-- want.yaml --
name: request-vars.txtar
requests:
  - vars:
      normal-var: test
    name: MyRequest
    method: GET
    url: https://example.com
    timeout: 30s
    connectionTimeout: 10s
    noRedirect: true
