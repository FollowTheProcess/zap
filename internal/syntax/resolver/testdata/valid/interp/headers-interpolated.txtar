name: interp/headers-interpolated.txtar
vars:
  accept: application/json;charset=utf8
  json: json
  something: "yes"
requests:
  - headers:
      Accept:
        - application/json;charset=utf8
      Content-Length:
        - "69"
      Content-Type:
        - application/json
      X-Something:
        - hello-yes-middle
    name: '#1'
