# Using a series of dense interps in one variable

-- src.http --
@base = https://example.com
@endpoint = users
@version = v1
@fullURL = {{ base }}/{{ version }}/{{ endpoint }}
-- want.yaml --
name: interp/dense-interps.txtar
vars:
  base: https://example.com
  endpoint: users
  fullURL: https://example.com/v1/users
  version: v1
